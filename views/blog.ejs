                  <a href="/like?type=coeur&couleur=rouge">  
                    Like <img src="/images/noun_Heart_3429586-svg.svg" background-color="">
                  </a>
                
                <h5 class="card-title"><%= objetApiStrapi[i].Film_title %></h5>
                <p class="card-text"><%= objetApiStrapi[i].Film_description%></p>
              </div>
            </div>
         </div>
          <%
           }
           %>
        </div>
      </div>
 
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
